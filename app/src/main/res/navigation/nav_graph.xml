<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/MainFragment">

    <fragment
        android:id="@+id/MainFragment"
        android:name="com.example.demo.fragment.MainFragment"
        tools:layout="@layout/fragment_first">
        <action
            android:id="@+id/action_MainFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
        <action
            android:id="@+id/action_MainFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
        <action
            android:id="@+id/action_MainFragment_to_StartActivityTestFragment"
            app:destination="@id/StartActivityTestFragment" />
    </fragment>

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.demo.fragment.FirstFragment"
        tools:layout="@layout/fragment_first" />
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.example.demo.fragment.SecondFragment"
        tools:layout="@layout/fragment_second" />

    <fragment
        android:id="@+id/StartActivityTestFragment"
        android:name="com.example.demo.fragment.StartActivityTestFragment"
        tools:layout="@layout/fragment_start_activity_test" />


</navigation>